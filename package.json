    "@dc0de/eslint-config-base": "^0.4.0",
    "@dc0de/eslint-config-node": "^0.4.9",
    "@dc0de/eslint-config-react": "^0.5.0",
    "@dc0de/jest-preset": "^0.0.8",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.20",
